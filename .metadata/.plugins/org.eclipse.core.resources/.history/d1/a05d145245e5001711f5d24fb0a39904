package lavanya;

public class Array {
	public static void main(String[] args)
	{
		int[] a= {7,5,6,3,1,4,8};
		int temp;
		int i;
		
	for(i=0;i<a.length;i++)
	{
	for(int j=i+1;j<a.length;j++)
	{
		if(a[i]>a[j])
		{
			temp=a[i];
			a[i]=a[j];
			a[j]=temp;
		}
	}
	}
	
	for(int k=0;k<a.length;k++)
	{
		int p=1;
		if(a[k]!=p)
		{
			System.out.println(k);
		}
	}
	
	
	}
}

	


