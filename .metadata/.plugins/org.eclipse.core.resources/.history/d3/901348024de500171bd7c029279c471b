
import java.util.Scanner;
public class Sorted {
	
	public static void main(String[] args) {
		int[] a= new int[5];
		int[] b=new int[5];
		
		Scanner s1= new Scanner(System.in);
		for (int i = 0; i < a.length; i++) {
			System.out.print("Enter the element of a"+a[i]);
			a[i]=s1.nextInt();
			
		}
        for (int i = 0; i < b.length; i++) {
        	System.out.print("Enter the element of b"+b[i]);
		
			a[i]=s1.nextInt();
        }	
		for (int i = 0; i < a.length; i++) {
			b[i+a.length]=a[i];		
				System.out.println(b[i]);
			}
			
		}

		
	}


