package lavanya;

public class Array {
	public static void main(String[] args)
	{
		int[] a= {7,5,6,3,2,4,8};
		int temp;
	for(int i=0;i<a.length;i++)
		{
			for(int j=i+1;j<a.length;j++)
			{
			if(a[i]<a[j])
			{
				temp=a[i];
				a[i]=a[j+1];
				a[j+1]=temp;
			}
				
			}
			System.out.println(a[i]);
		}
	
		
	}

}
